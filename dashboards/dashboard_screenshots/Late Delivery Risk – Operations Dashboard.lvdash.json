{
  "datasets": [
    {
      "name": "d06fa71f",
      "displayName": "Shipment Delivery Risk Totals",
      "queryLines": [
        "USE CATALOG `workspace`;\n",
        "USE SCHEMA `default`;\n",
        "\n",
        "SELECT COUNT(*) AS total_shipments\n",
        "FROM logistics_gold.delivery_risk_decisions;"
      ]
    },
    {
      "name": "bf56f3fd",
      "displayName": "Untitled dataset",
      "queryLines": [
        "USE CATALOG `workspace`;\n",
        "USE SCHEMA `default`;\n",
        "\n",
        "SELECT risk_category, COUNT(*) AS shipments\n",
        "FROM logistics_gold.delivery_risk_decisions\n",
        "GROUP BY risk_category;"
      ]
    },
    {
      "name": "9fa9880a",
      "displayName": "Untitled dataset",
      "queryLines": [
        "USE CATALOG `workspace`;\n",
        "USE SCHEMA `default`;\n",
        "\n",
        "SELECT\n",
        "  risk_category,\n",
        "  AVG(is_late) AS actual_delay_rate\n",
        "FROM logistics_gold.delivery_risk_decisions\n",
        "GROUP BY risk_category;"
      ]
    },
    {
      "name": "565f9207",
      "displayName": "Top Delivery Risks and Actions for Shipments",
      "queryLines": [
        "USE CATALOG `workspace`;\n",
        "USE SCHEMA `default`;\n",
        "\n",
        "SELECT shipment_id, delay_risk_score, recommended_action\n",
        "FROM logistics_gold.delivery_risk_decisions\n",
        "WHERE risk_category = 'High'\n",
        "ORDER BY delay_risk_score DESC\n",
        "LIMIT 15;"
      ]
    }
  ],
